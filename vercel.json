{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/search/(?<keyword>[^/]+)/(?<location>[^/]+)",
      "dest": "/search/[keyword]/[location]?keyword=$keyword&location=$location"
    },
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
